const pumpkinSpice = money => {
  var items = [0,0,0,0];
  let funds = money;


  while (funds >= 5) {
  funds -= 5;
  items[0]++;
  
}
  while (funds >= 3) {
  funds -= 3;
  items[1]++;
  
}
  while (funds >= 1) {
  funds -= 1;
  items[2]++;
  
}

items[3] = (items[0] * 30) + (items[1] * 15) + (items[2] * 3);
  return items;
};
